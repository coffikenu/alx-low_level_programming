#include <stdio.h>
#include "main.h"
/**
 * print_alphabet - check the code
 *
 * Return:no return
 */
void print_alphabet(void)
{
char alpha[26] = "abcdefghijklmnopqrstuvwxyz";
int i = 0;
 int j = 0;
while (i < 10)
{
while (j < 26)
{
putchar(alpha[i]);
if (j == 25)
{
putchar('\n');
}
j++
}
i++;
}
}
