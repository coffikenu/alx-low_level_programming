#include "main.h"
#include <stdlib.h>
/**
 * create_array - return array
 * @c: first parametr
 * @size: second parametr
 *
 * Return: Nothing.
 */
char *create_array(unsigned int size, char c)
{
char *p;
if (size <= 0)
{
p = NULL;
}
else if (size > 0)
{
p = (char*)(malloc(sizeof(c) * size); 
}
return (p);
}
