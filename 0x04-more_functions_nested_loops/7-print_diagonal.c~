#include <stdio.h>
#include "main.h"
/**
 * print_diagonal - check the code
 * @n: is parametr
 * Return: Always 0.
 */
void print_diagonal(int n)
{
int i;
int j;
for (i = 1; i <= n-1; i++)
{
for(j = 1; j <= i; j++)
{
if (j == i)
{
putchar('\\');
putchar('\n');
}
else if (j < i)
{
putchar(' ');
}
}
}
if (n <= 0)
{
putchar('\n');
}
}
